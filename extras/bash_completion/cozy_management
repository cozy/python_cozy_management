###-begin-cozy_management-completion-###
#
# cozy_management command completion script
#
# Installation: cozy_management completion >> ~/.bashrc  (or ~/.zshrc)
# Or, maybe: cozy_management completion > /usr/local/etc/bash_completion.d/cozy_management
#

_cozy_management()
{
	local cur prev opts
	COMPREPLY=()
	cur="${COMP_WORDS[COMP_CWORD]}"
	prev="${COMP_WORDS[COMP_CWORD-1]}"
	opts="--help -h backup clean_links compare_version create_token delete_token fix_oom_scores generate_certificate install_cozy install_requirements get_admin get_couchdb_admins get_cozy_param get_crt_common_name get_oom_scores is_cozy_registered make_links normalize_cert_dir ping_couchdb reset_token show_diag show_reporting unregister_cozy restore rebuild_app rebuild_all_apps migrate_2_node4 wait_cozy_stack emulate_smtp install_weboob update_weboob update_weboob_modules"

	COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
	return 0
}

complete -F _cozy_management cozy_management
###-end-cozy_management-completion-###
